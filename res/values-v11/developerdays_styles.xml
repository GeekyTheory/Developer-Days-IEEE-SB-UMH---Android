<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AutoCompleteTextViewdeveloperdays" parent="android:Widget.Holo.Light.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/list_selector_holo_light</item>
        <item name="android:background">@drawable/edit_text_holo_light</item>
    </style>

    <style name="Buttondeveloperdays" parent="android:Widget.Holo.Light.Button">
        <item name="android:background">@drawable/btn_default_holo_light</item>
    </style>

    <style name="ImageButtondeveloperdays" parent="android:Widget.Holo.Light.ImageButton">
        <item name="android:background">@drawable/btn_default_holo_light</item>
    </style>

    <style name="Spinnerdeveloperdays" parent="android:Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/list_selector_holo_light</item>
    </style>

    <style name="ProgressBardeveloperdays" parent="android:Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/progress_indeterminate_horizontal_holo_light</item>
    </style>

    <style name="RatingBardeveloperdays" parent="android:Widget.Holo.Light.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_full_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/ratingbar_full_holo_light</item>
    </style>

    <style name="CustomTabPageIndicator" parent="Widget.TabPageIndicator">
        <item name="android:background">@drawable/tab_indicator_ab_actionbar_developerdays_white</item>
        <item name="android:textAppearance">@style/CustomTabPageIndicator.Text</item>
        <item name="android:textColor">@color/dd_tabindicator_text</item>
        <item name="android:padding">0dp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="CustomTabPageIndicator.Text" parent="android:TextAppearance.Medium">
        <item name="android:typeface">normal</item>
    </style>

</resources>